#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    int n , test = 1;
    cout << "Enter the Integer : "
    scanf("%d",&n);
    if(n & test)
        cout << "Odd" << endl;
    else
        cout << "Even " << endl;
}
